

CREATE TABLE event(
eno NUMBER(4)
    CONSTRAINT EVE_ENO_PK PRIMARY KEY,
list VARCHAR2(50)
    CONSTRAINT EVE_LIS_NN NOT NULL,
content VARCHAR2(4000)
    CONSTRAINT EVE_CON_NN NOT NULL,
sday DATE,
eday DATE,
location VARCHAR2(20),
img VARCHAR2(20)
)


CREATE TABLE member(
mno NUMBER(4)
    CONSTRAINT MEMB_NO_PK PRIMARY KEY,
name VARCHAR2(20)
    CONSTRAINT MEMB_NAME_NN NOT NULL,
id VARCHAR2(20)
    CONSTRAINT MEMB_ID_UK UNIQUE,
pw VARCHAR2(20) 
    CONSTRAINT MEMB_PW_NN NOT NULL,
email VARCHAR2(30)
    CONSTRAINT MEMB_MAIL_UK UNIQUE,
gen CHAR(1)
    CONSTRAINT MEMB_GEN_NN NOT NULL
    CONSTRAINT MEMB_GEN_CK CHECK(gen IN('M', 'F')),
jdate DATE,
birth CHAR(10)
    CONSTRAINT MEMB_BIR_NN NOT NULL,
elist NUMBER(4)
    CONSTRAINT MEMB_EL_FK REFERENCES event(eno),
isShow CHAR(1)
    CONSTRAINT MEMB_IS_CK CHECK(isShow IN('Y', 'N'))
)

CREATE TABLE board(
bno NUMBER(6)
    CONSTRAINT BRD_BNO_PK PRIMARY KEY,
wmno NUMBER(4)
    CONSTRAINT BRD_MNO_FK REFERENCES member(mno)
    CONSTRAINT BRD_MNO_NN NOT NULL,
title VARCHAR2(50)
    CONSTRAINT BRD_TIT_NN NOT NULL,
body VARCHAR2(1000)
    CONSTRAINT BRD_BODY_NN NOT NULL,
wdate DATE,
upno NUMBER(6)
    CONSTRAINT BRD_UNO_FK REFERENCES board(bno)
)