ALTER TABLE EXPLACE
ADD EXPDIR varchar2(50);

ALTER TABLE EXPLACE
ADD EXPIMGNAME varchar2(50);

-- EXPDIR 이미지 경로 데이터입력
update EXPLACE set EXPDIR = '/moa/img/place/' where expno >= 1001;

